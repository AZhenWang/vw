create table focus_stocks(
   id INT NOT NULL AUTO_INCREMENT,
   code_id INT NOT NULL,
   created_at timestamp not null default CURRENT_TIMESTAMP,
   PRIMARY KEY(id),
   index(`code_id`)
)ENGINE=MyISAM DEFAULT CHARSET=utf8 comment'关注的股票';

create table rate_yearly(
   id INT NOT NULL AUTO_INCREMENT,
   code_id INT NOT NULL,
   date_id INT NOT NULL,
   plan_number int not null,
   rate_yearly float not null,
   min_rate float not null,
   max_rate float not null,
   mean_rate float not null,
   created_at timestamp not null default CURRENT_TIMESTAMP,
   PRIMARY KEY(id),
   index(`code_id`),
   index(`plan_number`),
   index `code_date_idx`(`date_id`, `code_id`)
)ENGINE=MyISAM DEFAULT CHARSET=utf8 comment'股票年收益率';